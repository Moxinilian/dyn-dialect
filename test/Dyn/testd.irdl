// RUN: dyn-opt %s | dyn-opt | FileCheck %s

irdl.dialect testd {
  // CHECK: irdl.type parametric {
  // CHECK:   irdl.parameters(param: irdl.Any)
  // CHECK: }
  irdl.type parametric {
    irdl.parameters(param: irdl.Any)
  }

  // CHECK: irdl.operation eq {
  // CHECK:   irdl.results(res: i32)
  // CHECK: }  
  irdl.operation eq {
    irdl.results(res: i32)
  }

  // CHECK: irdl.operation anyof {
  // CHECK:   irdl.results(res: irdl.AnyOf<i32, i64>)
  // CHECK: }
  irdl.operation anyof {
    irdl.results(res: irdl.AnyOf<i32, i64>)
  }

  // CHECK: irdl.operation any {
  // CHECK:   irdl.results(res: irdl.Any)
  // CHECK: }
  irdl.operation any {
    irdl.results(res: irdl.Any)
  }

  // CHECK: irdl.operation dynparams {
  // CHECK:   irdl.results(res: testd.parametric<irdl.AnyOf<i32, i64>>)
  // CHECK: }
  irdl.operation dynparams {
    irdl.results(res: testd.parametric<irdl.AnyOf<i32, i64>>)
  }

  // CHECK: irdl.operation params {
  // CHECK:   irdl.results(res: std.complex<irdl.AnyOf<i32, i64>>)
  // CHECK: }
  irdl.operation params {
    irdl.results(res: std.complex<irdl.AnyOf<i32, i64>>)
  }
}
